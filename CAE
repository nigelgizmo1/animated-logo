<script type="text/javascript">

	var path_C = document.getElementById("path_C");
	var path_A = document.getElementById("path_A");
	var path_E = document.getElementById("path_E");
	var cir_Dot = document.getElementById("cir_Dot");

	path_C.addEventListener("animationend", finished);
	path_A.addEventListener("animationend", finished);
	path_E.addEventListener("animationend", finished);
	cir_Dot.addEventListener("animationend", finished);

			function finished() {
				path_C.classList.add ("fin");
				path_A.classList.add("fin");
				path_E.classList.add ("fin");
				cir_Dot.classList.add("fin");
			}

</script>
